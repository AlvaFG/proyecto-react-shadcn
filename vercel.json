{
  "buildCommand": "npm run build:prod",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://comedorback.azurewebsites.net/api/:path*"
    },
    {
      "source": "/locations/:path*",
      "destination": "https://comedorback.azurewebsites.net/locations/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
